Proceso Ejercicio19
	// Ejercicio19
	// Pedir dos números y decir si uno es múltiplo del otro.
	
	// DEFINICION VARIABLES
	Definir num1, num2 Como Entero;
	
	// ENTRADA DATOS
	Escribir "Digite el num1 y el num2";
	leer num1,num2;
	
	// ASIGNACION DATOS/SALIDA
	si (num1 MOD num2 = 0)Entonces
		Escribir "El numero es multiplo del otro";
	SiNo
		Escribir "El numero no es multiplo del otro";
		
	FinSi
	
FinProceso
